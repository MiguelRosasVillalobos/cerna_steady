FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-4; // Ajusta según sea necesario

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 1e-4; // Ajusta según sea necesario
    }
    outlet
    {
        type            zeroGradient;
    }
    Wall
    {
        type            fixedValue; // Spalart-Allmaras utiliza fixedValue en lugar de funciones de pared específicas.
        value           uniform 0; // La viscosidad turbulenta en la pared es cero.
    }
}



// ************************************************************************* //
